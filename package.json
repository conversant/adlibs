    "doc": "jsdoc2md -t doc/README.hbs 'lib/**/*.js' > README.md; echo"
  },
  "engines": {
    "node": ">=14.4.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conversant/adlibs.git"
  },
  "keywords": [
    "util",
    "modules",
    "cross-browser"
  ],
  "author": "Conversant Media",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^3.1.0",
    "expect.js": "^0.3.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc-to-markdown": "^7.0.0",
    "jshint": "^2.12.0",
    "karma": "^6.2.0",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-safarinative-launcher": "^1.0.0",
    "karma-sauce-launcher": "^4.3.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-verbose-summary-reporter": "0.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.3.1",
    "node-libs-browser": "^2.2.1",
    "request": "^2.88.2",
    "sinon": "^9.2.4",
    "webpack": "^5.24.4"
  },
  "bugs": {
    "url": "https://github.com/conversant/adlibs/issues"
  },
  "homepage": "https://github.com/conversant/adlibs",
